{
	"name": "Airflow DevContainer",
	"dockerComposeFile": [
		"../airflow-docker-compose.yml",
		"docker-compose.yml"
	],
	"remoteUser": "airflow",
	"service": "airflow-scheduler",
	"workspaceFolder": "/workspaces/b3",
	"runServices": [
		"postgres",
		"airflow-init",
		"airflow-webserver",
		"airflow-scheduler"
	],
	"forwardPorts": [
		8080
	],
	"shutdownAction": "stopCompose",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-vscode-remote.remote-containers"
			],
			"settings": {
				"terminal.integrated.shell.linux": "/bin/bash"
			}
		}
	},
	"initializeCommand": "mkdir -p dags logs plugins"
}